{
  "name": "@oxygenium/sdk",
  "version": "0.7.6",
  "description": "A JS/TS library to interact with the Oxygenium platform",
  "license": "GPL",
  "main": "dist/index.js",
  "browser": "dist/oxygenium.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/oxygenium.min.js",
      "default": "./dist/index.js"
    }
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git@github.com:oxygenium/js-sdk.git"
  },
  "homepage": "https://github.com/oxygenium-network/js-sdk",
  "bugs": {
    "url": "https://github.com/oxygenium-network/js-sdk/issues"
  },
  "author": "Oxygenium dev <dev@oxygenium.org>",
  "scripts": {
    "build": "rm -rf dist && npx tsc --build . && webpack",
    "dev": "tsnd --respawn lib/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "jest": "jest --useStderr --silent=false --verbose=true --config jestconfig.json",
    "test": "npm run lint && npm run build && npm run jest",
    "test:watch": "npm run build && npm run jest -- --watch",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "devnet:start": "npx @oxygenium/cli devnet start"
  },
  "dependencies": {
    "@oxygenium/token-list": "^0.0.7",
    "@oxygenium/web3": "^0.12.2",
    "base-x": "^4.0.0",
    "bcfg": "~0.1.6",
    "bip32": "^2.0.6",
    "bip39": "^3.0.4",
    "blakejs": "^1.1.1",
    "cross-fetch": "^3.1.5",
    "crypto-js": "^4.1.1",
    "elliptic": "^6.5.4"
  },
  "devDependencies": {
    "@oxygenium/cli": "^0.10.2",
    "@oxygenium/web3-test": "^0.10.2",
    "@types/crypto-js": "^4.1.1",
    "@types/elliptic": "^6.4.13",
    "@types/jest": "^29.4.0",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.7.8",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "babel-eslint": "^10.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-localstorage-mock": "^2.4.18",
    "mock-fs": "^5.1.2",
    "mock-socket": "^9.0.8",
    "path-browserify": "^1.0.1",
    "prettier": "^2.3.2",
    "rewire": "^6.0.0",
    "shelljs": "^0.8.5",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^5.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
